 <% include ./partials/dashboardnavbar %>
 <form action='/entries/add' method="POST" enctype="multipart/form-data">
    <div class="form-group">
      <label for="exampleFormControlInput1">Title</label>
      <input type="text" name="title" class="form-control" id="exampleFormControlInput1" placeholder="Enter title" required>
    </div>
    <div class="form-group">
      <label for="exampleFormControlTextarea1">Diary</label>
      <textarea class="form-control" name="entry" placeholder="What all happened today?" id="exampleFormControlTextarea1" rows="5" required></textarea>
    </div>
    <div class="form-group">
      <label for="image">Attach an image if you need </label>
      <input type="file" class="form-control-file" name='picture' id="image" accept="image/*">
    </div>
    <div class="form-check">
      <input class="form-check-input" type="radio" name="visibility" id="exampleRadios1" value="private" checked>
      <label class="form-check-label" for="exampleRadios1">
        Add as private
      </label>
    </div>
    <div class="form-check">
      <input class="form-check-input" type="radio" name="visibility" id="exampleRadios2" value="public">
      <label class="form-check-label" for="exampleRadios2">
        Add as public
      </label>
    </div>
    <button type="submit" class="btn btn-primary">Add diary</button>
  </form>
